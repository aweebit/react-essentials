      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^18.3.24",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "3.6.2",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "license": "MIT"
