{
  "name": "@aweebit/react-essentials",
  "version": "0.10.0",
  "type": "module",
  "repository": "github:aweebit/react-essentials",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prettier:base": "prettier --write",
    "prettier": "npm run prettier:base -- .",
    "lint": "eslint --max-warnings=0",
